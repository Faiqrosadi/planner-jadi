{{ content|safe }}
<div class="column-sidebar sidebar-baru">
  {% if index_page %}
  <a style="z-index:13" href="#index-page">
<svg width="20" height="45" viewBox="0 0 20 45" fill="none" xmlns="http://www.w3.org/2000/svg">
<path d="M4.48649 1C11.9498 1 18 7.05021 18 14.5135L18 30.4865C18 37.9498 11.9498 44 4.48649 44H1L1 1L4.48649 1Z" fill="#CBCBCB"/>
<path d="M4.48649 0C11.9498 0 18 6.05021 18 13.5135L18 29.4865C18 36.9498 11.9498 43 4.48649 43H1L1 0L4.48649 0Z" fill="#FFFFFF"/>
<circle cx="9.34366" cy="21.3435" r="2.08827" transform="rotate(91.1833 9.34366 21.3435)" fill="#626463"/>
<circle cx="9.34387" cy="21.3436" r="4.08812" transform="rotate(90 9.34387 21.3436)" stroke="#626463" stroke-width="0.511026"/>
<rect x="1" width="43" height="1" transform="rotate(90 1 0)" fill="#FFFFFF"/>
</svg>
  </a>
  {% else %}
  <a style="z-index:13" href="#index-page">
<svg width="20" height="45" viewBox="0 0 20 45" fill="none" xmlns="http://www.w3.org/2000/svg">
<path d="M4.48649 1C11.9498 1 18 7.05021 18 14.5135L18 30.4865C18 37.9498 11.9498 44 4.48649 44H1L1 1L4.48649 1Z" fill="#CBCBCB"/>
<path d="M4.48649 0C11.9498 0 18 6.05021 18 13.5135L18 29.4865C18 36.9498 11.9498 43 4.48649 43H1L1 0L4.48649 0Z" fill="#DEDEDE"/>
<circle cx="9.34366" cy="21.3435" r="2.08827" transform="rotate(91.1833 9.34366 21.3435)" fill="#A0907F"/>
<circle cx="9.34387" cy="21.3436" r="4.08812" transform="rotate(90 9.34387 21.3436)" stroke="#A0907F" stroke-width="0.511026"/>
<rect x="1" width="44" height="1" transform="rotate(90 1 0)" fill="#CBCBCB"/>
</svg>
</a>
  {% endif %}
  {% for sidebar_bulan in sidebar_bulan_list %}
  {% if date %}
    {% if sidebar_bulan[0][7] == date.strftime('%b').upper() %}
    <a style="z-index:999" class="svg-baru-banget" href="#{{sidebar_bulan[0][8]}}-month-overview">
      <div class="grid-baru">
          <div  class="layer1 kotak-aktif">
          </div>
          <div class="layer2 flex items-center">
            <div class="sidebar-text">
            <div style="color:#626463; font-size:7.72px; text-align:center">{{sidebar_bulan[0][7]}}</div>
            </div>
          </div>
      </div>
    </a>
  {% else %}
    <a style="z-index:{{13 - loop.index}}" class="svg-baru-banget" href="#{{sidebar_bulan[0][8]}}-month-overview">
        <div class="grid-baru">
          <div  class="layer1 flex kotak-non-aktif">
          </div>
          <div class="layer2 flex items-center">
            <div class="sidebar-text">
            <div style="color:#A0907F; font-size:7.72px;text-align:center">{{sidebar_bulan[0][7]}}</div>
            </div>
          </div>
      </div>
    </a>
  {% endif %}
  {% else %}
    <a style="z-index:{{13 - loop.index}}" class="svg-baru-banget" href="#{{sidebar_bulan[0][8]}}-month-overview">
        <div class="grid-baru">
          <div class="layer1 kotak-non-aktif">
          </div>
          <div class="layer2 flex items-center">
            <div class="sidebar-text">
            <div style="color:#A0907F; font-size:7.72px;text-align:center">{{sidebar_bulan[0][7]}}</div>
            </div>
          </div>
      </div>
    </a>
  {% endif%}
  {% endfor %}
</div>
<div class="footer">
    <div class="footer-teks"><span style="font-size: 5px">&#169 </span>LARASNUGH</div>
</div>
</div>